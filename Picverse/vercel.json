{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/",
      "destination": "/htmlCode/homepage.html"
    },
    {
      "source": "/html/:path*",
      "destination": "/htmlCode/:path*"
    },
    {
      "source": "/css/:path*",
      "destination": "/CssCode/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/JsCode/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/Asset/:path*"
    }
  ]
}